#!/usr/bin/env bash
# ----------------------------------------------------------------------
# Description
#   Print a summary of the proxy-related environment variables.
# ----------------------------------------------------------------------

echo "PROXY_HOST=${PROXY_HOST:-<unset>}"
echo "PROXY_PORT=${PROXY_PORT:-<unset>}"
echo "NON_PROXY_HOSTS=${NON_PROXY_HOSTS:-<unset>}"
echo "http_proxy=${http_proxy:-<unset>}"
echo "https_proxy=${https_proxy:-<unset>}"
echo "ftp_proxy=${ftp_proxy:-<unset>}"
