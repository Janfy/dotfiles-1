  <!-- Active profiles -->
  <activeProfiles>
    <activeProfile>repositories-flex</activeProfile>
    <activeProfile>repositories-OW2</activeProfile>
    <activeProfile>repositories-JBoss</activeProfile>
    <activeProfile>repositories-java-net</activeProfile>
